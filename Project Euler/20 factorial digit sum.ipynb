   "source": [
    "def fac(n) :\n",
    "    if n == 1 :\n",
    "        return 1\n",
    "    return n*fac(n-1)\n",
    "fac(100)\n",
    "\n",
    "s = str(fac(100))\n",
    "\n",
    "result = 0 \n",
    "for x in s : \n",
    "    result += int(x)\n",
    "    \n",
    "result"
   ]
  }
