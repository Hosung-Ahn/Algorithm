    "def is_palin(s) :\n",
    "    for i in range(len(s)//2) :\n",
    "        if s[i] != s[-(i+1)] : return False \n",
    "    return True\n",
    "\n",
    "SUM = 0\n",
    "\n",
    "for n in range(1,1_000_000+1) :\n",
    "    if is_palin(str(n)) and is_palin(bin(n)[2:]) :\n",
    "        print(n)\n",
    "        SUM += n\n",
    "print(SUM)"
   ]
  }
