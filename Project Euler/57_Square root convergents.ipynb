    "def recur(cnt) :\n",
    "    if cnt == 1 : return (1,2)\n",
    "    n,d = recur(cnt-1)\n",
    "    return (d, 2*d + n)\n",
    "\n",
    "ans = 0\n",
    "for i in range(1,1001) :\n",
    "    n,d = recur(i)\n",
    "    if len(str(n+d)) > len(str(d)) : ans += 1\n",
    "    \n",
    "ans"
   ]
  }
